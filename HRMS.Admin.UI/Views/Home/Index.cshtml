@{
    ViewData["Title"] = "Home Page";
}
@using System.Globalization;
<link href="https://beats.squareyards.com//resources/dist/css/profile-view.css" rel="stylesheet" />
<style>
    .content {
         min-height: 0px; 
        padding: 15px;
        margin-right: auto;
        margin-left: auto;
        padding-left: 15px;
        padding-right: 15px;
    }
</style>
<div class="content-wrapper" style="min-height: 870px;">
