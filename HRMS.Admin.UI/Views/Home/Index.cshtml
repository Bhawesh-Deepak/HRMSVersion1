                <div class="small-box bg-success">
                    <div class="inner">
                        <h3>53</h3>

                        <p>
                            ACTIVE EMPLOYEE
                        </p>
                    </div>
                    <div class="icon">
                        <i class="fa fa-user-plus"></i>
                    </div>
                    <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
