@model HRMS.Core.Entities.HR.PaidRegister


<div class="content-wrapper" style="min-height: 870px;">

    <section class="content">


        <div class="row">

            <div class="col-md-12">
                <div class="row" style="margin-top:0;">
                    <div class="col-md-12 col-sm-12">
                        <div class="box box-primary">
                            <div class="box-header with-border">
                                <div class="box-title">
                                    <h3 class="box-title pull-left">List of Paid Register </h3>

                                </div>
                                <a class="btn btn-primary pull-right btn-xs" onclick="AddRecord()" title="Add New Paid Register">
                                    <i class="fa fa-plus"> &nbsp; Add</i>
                                </a>
                            </div>
                            <div class="box-body">
                                <div id="divHRMS">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>


    </section>
</div>

<environment include="Development">
    <script src="~/HRMSJs/PaidRegisterUpload.js"></script>
</environment>
<environment include="Production">
    <script src="~/HRMSJs/PaidRegisterUpload.js"></script>
</environment>


<script>
    function checkfile(sender) {
        var validExts = new Array(".xlsx", ".xls");
        var fileExt = sender.value;
        var filename = $('#UploadFilePath')[0].files[0].name;
        fileExt = fileExt.substring(fileExt.lastIndexOf('.'));
        if (validExts.indexOf(fileExt) < 0) {
            $('#UploadFilePath').val(null);
            $('#span_excelValidate').show('slow');
            $('#span_excelValidate').text("Invalid file selected, valid files are of " + validExts.toString() + " types.");
            return false;
        }
        else {

            $('#span_excelValidate').hide('slow');
            return true;
        }



    }
</script>